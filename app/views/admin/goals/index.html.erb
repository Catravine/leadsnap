<h1>Current Goals</h1>
<ul>
  <% @goals.current.each do |goal| %>
    <li><%= render partial: 'list', locals: { goal: goal } %></li>
  <% end %>
</ul>

<h3>Previous Goals</h3>
<ul>
  <% @goals.expired.each do |goal| %>
    <li><%= render partial: 'list', locals: { goal: goal } %></li>
  <% end %>
</ul>
